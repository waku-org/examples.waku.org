{
  "name": "eth-pm",
  "version": "0.1.0",
  "private": true,
  "homepage": "/eth-pm",
  "dependencies": {
    "@ethersproject/abstract-signer": "5.7.0",
    "@ethersproject/providers": "5.7.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "ethers": "5.7.1",
    "fontsource-roboto": "^4.0.0",
    "@waku/create": "0.0.4",
    "@waku/core": "0.0.6",
    "@waku/interfaces": "0.0.5",
    "@waku/byte-utils": "0.0.2",
    "@waku/message-encryption": "0.0.4",
    "protobufjs": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uint8arrays": "^4.0.2"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false PORT=3004 craco start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "fix": "run-s fix:*",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts --ext .tsx",
    "test:prettier": "prettier \"src/**/*.{ts,tsx}\" \"./*.json\" --list-different",
    "test:spelling": "cspell \"{README.md,src/**/*.{ts,tsx},public/**/*.html}\" -c ../.cspell.json",
    "fix:prettier": "prettier \"src/**/*.{ts,tsx}\" \"./*.json\" --write",
    "fix:lint": "eslint src --ext .ts --ext .tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@ethersproject/shims": "^5.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "cspell": "^6.0.0",
    "eslint": "^8.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.2"
  }
}
